<!DOCTYPE tiles-definitions PUBLIC
"-//Apache Software Foundation//DTD Tiles Configuration 2.0//EN" 
"http://struts.apache.org/dtds/tiles-config_2_0.dtd">

<tiles-definitions>
    <definition name = "welcome" template = "/mylayout.jsp">
        <put-attribute name = "title" value = "welcome Page"/>
        <put-attribute name = "header" value = "/h1.jsp"/>
        <put-attribute name = "footer" value = "/f1.jsp"/>
        <put-attribute name = "body" value = "/welcome.jsp"/>
    </definition>
    
    <definition name = "relogin" template = "/mylayout.jsp">
      <put-attribute name = "title" value = "Login Error"/>
        <put-attribute name = "header" value = "/h1.jsp"/>
